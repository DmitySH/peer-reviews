<Window x:Class="DataAnalyzer.HistConfig"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:local="clr-namespace:DataAnalyzer"
        mc:Ignorable="d"
        Title="HistConfig" Height="275" Width="328">
    <Grid>
        <xctk:IntegerUpDown  VerticalAlignment="Top" HorizontalAlignment="Left" Name="UpDown" Margin="10,0,0,0" Maximum="10" Minimum="1" Value="1" Height="28"/>
        <ListBox Name="Box" Margin="0,28,0,0">
        </ListBox>
        <Button Name="PlotButton" Content="Plot it" HorizontalAlignment="Left" VerticalAlignment="Top" Height="25" Width="57" Click="PlotButton_OnClick" Margin="247,2,0,0"/>
        <Label Content="Columns to merge" Margin="56,2,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Height="32"/>
    </Grid>
</Window>
